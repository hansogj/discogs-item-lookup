{
  "name": "@hansogj/discogs-item-lookup",
  "version": "1.1.1",
  "description": "A CLI and library to look up item details from the Discogs API.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "discogs-lookup": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsup",
    "cli": "node dist/cli.js",
    "test": "vitest run",
    "prepack": "pnpm format:check && pnpm test &&  pnpm build",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "discogs",
    "api",
    "cli",
    "music",
    "vinyl",
    "lookup"
  ],
  "author": "Hans Ole Gjerdrem",
  "license": "ISC",
  "dependencies": {
    "commander": "^12.0.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "^1.5.0",
    "msw": "^2.2.14",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hansogj/discogs-item-lookup.git"
  },
  "bugs": {
    "url": "https://github.com/hansogj/discogs-item-lookup/issues"
  },
  "homepage": "https://github.com/hansogj/discogs-item-lookup#readme"
}
